{%- unless settings.type_body_font.system?
  and settings.type_subheading_font.system?
  and settings.type_heading_font.system?
  and settings.type_accent_font.system?
  and settings.type_custom_font_1.system?
  and settings.type_custom_font_2.system?
  and settings.type_custom_font_3.system?
  and settings.type_custom_font_4.system?
  and settings.type_custom_font_5.system?
  and settings.type_custom_font_6.system?
-%}
  <link
    rel="preconnect"
    href="https://fonts.shopifycdn.com"
    crossorigin
  >
{%- endunless -%}

{% # theme-check-disable %}
{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
  >
{%- endunless -%}
